{
  "expo": {
    "name": "ShopLinkMobile",
    "slug": "shoplink-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.shoplink.mobile",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.shoplink.mobile"
    },
    "web": {
      "favicon": "./assets/logo.png",
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "a27eaee9-9f80-4bd3-a02f-23b1a5ef26ec"
      }
    }
  }
}
